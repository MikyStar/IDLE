{
  "compilerOptions": {
    "noEmit": true,
    "lib": [ "dom", "ESNext.AsyncIterable", "ES2017", "ES6" ],
    "types": ["node", "jest"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strict": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "preserve",
    "target": "esnext",
    "emitDecoratorMetadata": true,
    "noImplicitAny": false
  }
}
